<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Form Container"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
          <tabs
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/tabs"
            maximized="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
              <formProperties
                jcr:primaryType="nt:unstructured"
                jcr:title="Form Properties"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <columns
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                    margin="{Boolean}true">
                    <items jcr:primaryType="nt:unstructured">
                      <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                          <eyebrow jcr:primaryType="nt:unstructured" fieldLabel="Eyebrow" name="./eyebrow" sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                          <errorMessage jcr:primaryType="nt:unstructured" required="true" fieldLabel="Error Message" name="./errorMessage" sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                          <formHandler jcr:primaryType="nt:unstructured" required="true" fieldLabel="Form Handler" name="./formHandler" sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                          <formType jcr:primaryType="nt:unstructured"
                            required="true"
                            fieldLabel="Form Type"
                            name="./formType"
                            emptyOption="{Boolean}true"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select">
                            <items jcr:primaryType="nt:unstructured">
                              <option1 jcr:primaryType="nt:unstructured"
                                text="Apps Contact Sales in page form"
                                value="lead form: apps: contact sales: in page"/>
                              <option2 jcr:primaryType="nt:unstructured"
                                text="Apps Contact Sales lightbox form"
                                value="lead form: apps: contact sales: lightbox"/>
                              <option3 jcr:primaryType="nt:unstructured"
                                text="Apps Contact Sales landing page"
                                value="lead form: apps: contact sales: lp: in page"/>
                              <option4 jcr:primaryType="nt:unstructured"
                                text="Apps Contact Sales landing page lightbox"
                                value="lead form: apps: contact sales: lp: lightbox"/>
                              <option5 jcr:primaryType="nt:unstructured"
                                text="Apps Request a Demo in page form"
                                value="lead form: apps: request a demo: in page"/>
                              <option6 jcr:primaryType="nt:unstructured"
                                text="Apps Request a Demo lightbox form"
                                value="lead form: apps: request a demo: lightbox"/>
                              <option7 jcr:primaryType="nt:unstructured"
                                text="Apps Request a Demo landing page"
                                value="lead form: apps: request a demo: lp: in page"/>
                              <option8 jcr:primaryType="nt:unstructured"
                                text="Apps Request a Demo landing page lightbox"
                                value="lead form: apps: request a demo: lp: lightbox"/>
                              <option9 jcr:primaryType="nt:unstructured"
                                text="Apps Content download in page form"
                                value="lead form: apps: download: in page"/>
                              <option10 jcr:primaryType="nt:unstructured"
                                text="Apps Content download lightbox form"
                                value="lead form: apps: download: lightbox"/>
                              <option11 jcr:primaryType="nt:unstructured"
                                text="Apps Content download landing page"
                                value="lead form: apps: download: lp: in page"/>
                              <option12 jcr:primaryType="nt:unstructured"
                                text="Apps Content download landing page lightbox"
                                value="lead form: apps: download: lp: lightbox"/>
                              <option14 jcr:primaryType="nt:unstructured"
                                text="Apps webinar in page form"
                                value="lead form: apps: webinar: in page"/>
                              <option15 jcr:primaryType="nt:unstructured"
                                text="Apps webinar lightbox form"
                                value="lead form: apps: webinar: lightbox"/>
                              <option16 jcr:primaryType="nt:unstructured"
                                text="Apps webinar landing page"
                                value="lead form: apps: webinar: lp: in page"/>
                              <option17 jcr:primaryType="nt:unstructured"
                                text="Apps webinar landing page lightbox"
                                value="lead form: apps: webinar: lp: lightbox"/>
                              <option18 jcr:primaryType="nt:unstructured"
                                text="Apps enewsletter in page form"
                                value="enewsletter form: apps: sign up: in page"/>
                              <option19 jcr:primaryType="nt:unstructured"
                                text="Apps enewsletter lightbox form"
                                value="enewsletter form: apps: sign up: lightbox"/>
                              <option20 jcr:primaryType="nt:unstructured"
                                text="Apps enewsletter landing page"
                                value="enewsletter form: apps: sign up: lp: in page"/>
                              <option21 jcr:primaryType="nt:unstructured"
                                text="Apps enewsletter landing page lightbox"
                                value="enewsletter form: apps: sign up: lp: lightbox"/>
                              <option22 jcr:primaryType="nt:unstructured"
                                text="Apps partner in page form"
                                value="partner form: apps: in page"/>
                              <option23 jcr:primaryType="nt:unstructured"
                                text="Apps partner lightbox form"
                                value="partner form: apps: lightbox"/>
                              <option24 jcr:primaryType="nt:unstructured"
                                text="Apps partner landing page"
                                value="partner form: apps: lp: in page"/>
                              <option25 jcr:primaryType="nt:unstructured"
                                text="Apps partner landing page lightbox"
                                value="partner form: apps: lp: lightbox"/>
                              <option26 jcr:primaryType="nt:unstructured"
                                text="API Contact Sales in page form"
                                value="lead form: api: contact sales: in page"/>
                              <option27 jcr:primaryType="nt:unstructured"
                                text="API Contact Sales lightbox form"
                                value="lead form: api: contact sales: lightbox"/>
                              <option28 jcr:primaryType="nt:unstructured"
                                text="API Contact Sales landing page"
                                value="lead form: api: contact sales: lp: in page"/>
                              <option29 jcr:primaryType="nt:unstructured"
                                text="API Contact Sales landing page lightbox"
                                value="lead form: api: contact sales: lp: lightbox"/>
                              <option30 jcr:primaryType="nt:unstructured"
                                text="API Content download in page form"
                                value="lead form: api: download: in page"/>
                              <option31 jcr:primaryType="nt:unstructured"
                                text="API Content download lightbox form"
                                value="lead form: api: download: lightbox"/>
                              <option32 jcr:primaryType="nt:unstructured"
                                text="API Content download landing page"
                                value="lead form: api: download: lp: in page"/>
                              <option33 jcr:primaryType="nt:unstructured"
                                text="API Content download landing page lightbox"
                                value="lead form: api: download: lp: lightbox"/>
                              <option34 jcr:primaryType="nt:unstructured"
                                text="API webinar in page form"
                                value="lead form: api: webinar: in page"/>
                              <option35 jcr:primaryType="nt:unstructured"
                                text="API webinar lightbox form"
                                value="lead form: api: webinar: lightbox"/>
                              <option36 jcr:primaryType="nt:unstructured"
                                text="API webinar landing page"
                                value="lead form: api: webinar: lp: in page"/>
                              <option37 jcr:primaryType="nt:unstructured"
                                text="API webinar landing page lightbox"
                                value="lead form: api: webinar: lp: lightbox"/>
                              <option38 jcr:primaryType="nt:unstructured"
                                text="API enewsletter in page form"
                                value="enewsletter form: api: sign up: in page"/>
                              <option39 jcr:primaryType="nt:unstructured"
                                text="API enewsletter lightbox form"
                                value="enewsletter form: api: sign up: lightbox"/>
                              <option40 jcr:primaryType="nt:unstructured"
                                text="API enewsletter landing page"
                                value="enewsletter form: api: sign up: lp: in page"/>
                              <option41 jcr:primaryType="nt:unstructured"
                                text="API enewsletter landing page lightbox"
                                value="enewsletter form: api: sign up: lp: lightbox"/>
                              <option42 jcr:primaryType="nt:unstructured"
                                text="API partner in page form"
                                value="partner form: api: in page"/>
                              <option43 jcr:primaryType="nt:unstructured"
                                text="API partner lightbox form"
                                value="partner form: api: lightbox"/>
                              <option44 jcr:primaryType="nt:unstructured"
                                text="API partner landing page"
                                value="partner form: api: lp: in page"/>
                              <option45 jcr:primaryType="nt:unstructured"
                                text="API partner landing page lightbox"
                                value="partner form: api: lp: lightbox"/>
                            </items>
                          </formType>
                        </items>
                      </column>
                    </items>
                  </columns>
                </items>
              </formProperties>

              <webinarSettings
                jcr:primaryType="nt:unstructured"
                jcr:title="Webinar Settings"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <columns
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                    margin="{Boolean}true">
                    <items jcr:primaryType="nt:unstructured">
                      <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                          <webinarInstance
                            jcr:primaryType="nt:unstructured"
                            required="false"
                            fieldLabel="Webinar Instance"
                            name="./eventInstance"
                            emptyOption="{Boolean}true"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select">
                            <items jcr:primaryType="nt:unstructured">
                              <option1 jcr:primaryType="nt:unstructured"
                                text="Nexmo"
                                value="nexmo"/>
                              <option2 jcr:primaryType="nt:unstructured"
                                text="NewVoiceMedia"
                                value="nvm"/>
                            </items>
                          </webinarInstance>


                        <webinarType
                          jcr:primaryType="nt:unstructured"
                          required="false"
                          fieldLabel="Webinar Type"
                          name="./webinarType"
                          emptyOption="{Boolean}true"
                          sling:resourceType="granite/ui/components/coral/foundation/form/select">
                          <items jcr:primaryType="nt:unstructured">
                            <option1 jcr:primaryType="nt:unstructured"
                              text="Webcast"
                              value="webcast"/>
                            <option2 jcr:primaryType="nt:unstructured"
                              text="Virtual Event"
                              value="virtualEvent"/>
                          </items>
                        </webinarType>

                        <showCode
                        jcr:primaryType="nt:unstructured"
                        required="false"
                        fieldLabel="Show Code for Virtual Event"
                        name="./showCode"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                      />

                          <eventId
                            jcr:primaryType="nt:unstructured"
                            required="false"
                            fieldLabel="Event ID for Webcast"
                            name="./eventId"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                          />
                        </items>
                      </column>
                    </items>
                  </columns>
                </items>
              </webinarSettings>


            </items>
          </tabs>
        </items>
    </content>
</jcr:root>
