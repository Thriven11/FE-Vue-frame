	jcr:primaryType="nt:unstructured" jcr:title="Checkout Form"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<column jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/container">
				<items jcr:primaryType="nt:unstructured">
					<tabs jcr:primaryType="nt:unstructured" maximized="true"
						sling:resourceType="granite/ui/components/coral/foundation/tabs">
						<items jcr:primaryType="nt:unstructured">
							<account jcr:primaryType="nt:unstructured"
								jcr:title="Account"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<column jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
													<contactTitle
														jcr:primaryType="nt:unstructured"
														fieldLabel="Contact Title" name="./account/title"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
													<ServiceAddress
														jcr:primaryType="nt:unstructured"
														fieldLabel="Service Address" name="./account/address"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
													<ServiceDescription
														jcr:primaryType="nt:unstructured"
														fieldLabel="Service Description"
														name="./account/description"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
													<CompanyTitle
														jcr:primaryType="nt:unstructured"
														fieldLabel="Company Title"
														name="./account/companyAddress"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
													<CompanyDescription
														jcr:primaryType="nt:unstructured"
														fieldLabel="Use same address label"
														name="./account/companyDescription"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
													
												</items>
											</column>
										</items>
									</column>
								</items>
							</account>
							<shipping jcr:primaryType="nt:unstructured"
								jcr:title="Shipping"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<column jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
