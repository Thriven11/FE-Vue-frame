<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Add-On"
          sling:resourceType="cq/gui/components/authoring/dialog">
  <content
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/foundation/container">
    <items jcr:primaryType="nt:unstructured">
      <tabs jcr:primaryType="nt:unstructured" maximized="true" sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
          <textSection jcr:primaryType="nt:unstructured" jcr:title="Text Section" sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
              <columns jcr:primaryType="nt:unstructured" margin="true" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                  <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                      <title jcr:primaryType="nt:unstructured" fieldLabel="Title" name="./title" sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                      <subTitle jcr:primaryType="nt:unstructured" fieldLabel="SubTitle" name="./subTitle" sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
					  <headline
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Headline"
                                                name="./headline"/>
                    </items>
                  </column>
                </items>
              </columns>
            </items>
          </textSection>
		   <tabsSection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tabs section"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <tabOneLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="tab 1 Label"
                                                name="./tabOneLabel"/>
                                            <tabTwoLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="tab 2 Label"
                                                name="./tabTwolabel"/>
                                            <tabThreeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="tab 3 Label"
                                                name="./tabThreeLabel"/>
                                            <tabThreeSubProductOne
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Tab 3 Sub-Product One"
                                                name="./tabThreeSubProductOne"/>
                                            <tabThreeSubProductTwo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Tab 3 Sub-Product Two"
                                                name="./tabThreeSubProductTwo"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </tabsSection>
        </items>
      </tabs>
    </items>
  </content>
</jcr:root>
