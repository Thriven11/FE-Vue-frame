<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="cq:Page">
  <jcr:content
    jcr:primaryType="cq:PageContent"
    jcr:title="Attribution"
    sling:resourceType="wcm/scaffolding/components/scaffolding"
    cq:targetPath="/var/commerce/products/vonage/attribution-campaigns">
    <cq:dialog jcr:primaryType="nt:unstructured" jcr:title="We.Retail Product Properties" sling:resourceType="cq/gui/components/authoring/dialog">
      <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
          <columns jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
            <items jcr:primaryType="nt:unstructured">
              <column2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                  <title
                    jcr:primaryType="nt:unstructured"
                    name="./jcr:title"
                    fieldLabel="Campaign Name"
                    required="true"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                  <campaignTfn
                    jcr:primaryType="nt:unstructured"
                    name="./campaignTfn"
                    fieldLabel="Phone Number"
                    required="true"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                  <campaignId
                    jcr:primaryType="nt:unstructured"
                    name="./campaignId"
                    fieldLabel="Salesforce Campaign ID"
                    required="true"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                </items>
              </column2>
            </items>
          </columns>
        </items>
      </content>
    </cq:dialog>
  </jcr:content>
</jcr:root>
